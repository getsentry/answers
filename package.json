{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "name": "static-sites",
  "scripts": {
    "build:blog": "yarn workspace @sentry-static/blog run build",
    "build:www": "yarn workspace @sentry-static/www run build",
    "build:help": "yarn workspace @sentry-static/help run build",
    "build:open": "yarn workspace @sentry-static/open run build",
    "start:blog": "yarn workspace @sentry-static/blog run start",
    "start:www": "yarn workspace @sentry-static/www run start",
    "start:help": "yarn workspace @sentry-static/help run start",
    "start:open": "yarn workspace @sentry-static/open run start",
    "storybook": "yarn workspace @sentry-static/shared run storybook",
    "generate-csp": "node ./bin/generate-csp.js"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "command-line-args": "^5.2.1",
    "contentful-management": "^9.0.0",
    "csv-stringify": "^6.0.5",
    "memoizee": "^0.4.15"
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.11"
  }
}
