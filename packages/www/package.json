  "scripts": {
    "build": "gatsby clean && gatsby build --prefix-paths",
    "build-for-vercel": "rm -rf ../blog/ ../open/ ../help/ && yarn build && rm -rf .cache",
    "build-for-jekyll": "gatsby clean; rm -rf public; gatsby build --prefix-paths; rm -rf public/{admin,media}; find public -maxdepth 1 -type f -print0| xargs -0 rm; rm -rf ../../../sentry.io./src/_gatsby_generated_files; cp -r public/ ../../../sentry.io/src/_gatsby_generated_files",
    "develop": "gatsby develop --open --port 4000",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "GATSBY_GETSENTRY_URL=https://test.sentry.io/ jest",
    "prod": "gatsby build --prefix-paths && gatsby serve --open --port 4000",
    "csp": "node -e 'console.log(require(\"../shared/utils/csp\").header({cspAPI: \"1297627\", sentryKey: \"e811b9077ef64dcf8a279ec18a61b222\"}))'",
